                                    :field_name="'visit_report_doctor[foreign_worker_present]'" 
                                    :rules="foreignWorkerPresentRule"
                                    :label="'No Of Foreign Workers Present'" 
                                    :validation_label="'Comment'"
                                    :readonly="formReadOnly"
                                ></v-number>
                            
                            </div>

                            <div class="form-group">

                                <v-textarea
                                    v-model="visit_report_data.visit_report_doctor.foreign_worker_present_comment"
                                    :field_name="'visit_report_doctor[foreign_worker_present_comment]'"
                                    :rules="foreignWorkerPresentCommentRule"
                                    :label="'If unacceptable, please specify'" 
                                    :validation_label="'Comment'"
                                    :readonly="formReadOnly" 
                                ></v-textarea>
                            
