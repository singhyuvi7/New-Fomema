<div class="row">

        <div class="col-md-12">

        <div v-if="visit_report_data.status" class="alert alert-info" role="alert">
            LATEST STATUS: <b>{{ visit_report_data.status }}</b>
        </div>

        <div v-if="!canEdit" class="alert alert-warning" role="alert">
            FORM UPDATE IS DISABLE FOR STATUS: <b>{{ visit_report_data.status }}</b>
        </div>

        <form ref="visit_report_form" method="POST" action="<%= form_action %>">

            <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

            <% if form_method.downcase !='post' %>
                <input type="hidden" name="_method" value="<%= form_method %>">
            <% end %>

            <div class="form-row">
                <div class="col">
                    <div class="form-group">

                        <v-text 
                            v-model="visit_report_data.id"
                            :field_name="'visit_report[id]'" 
                            :label="'Report Id'" 
                            :readonly="true"
                        ></v-text>
                        
                    </div>
                </div>
            
            </div>

            <div class="form-row">
                <div class="col">
                    <div class="form-group">

                        <v-text
                            v-model="visit_report_data.visit_plan_item.visitable.clinic_name"
                            :field_name="'visit_report_doctor[clinic_name]'"
                            :label="'Clinic Name'"
                            :readonly="true"
                        ></v-text>
                        
                    </div>
                </div>

                <div class="col">
                    <div class="form-group">

                        <v-text
                            v-model="visit_report_data.visit_plan_item.visitable.code"
                            :field_name="'visit_report_doctor[code]'"
                            :label="'Doctor Code'"
                            :readonly="true"
                        ></v-text>
                        
                    </div>
                </div>

                <div class="col">
                    <div class="form-group">

                        <v-text
                            v-model="visit_report_data.visit_plan_item.visitable.name"
                            :field_name="'visit_report_doctor[doctor_name]'"
                            :label="'Doctor Name'"
                            :readonly="true"
                        ></v-text>
                
                    </div>
                </div>
            
            </div>

            <div class="form-row">
                <div class="col">
                    <div class="form-group">
                        <label class="required" for="">Type of Visit</label>
                        <multiselect
                        v-validate="categoryRule" 
                        :data-vv-as="'Category'" 
                        v-model="visit_report_data.visit_report_doctor.type_of_visit"
                        :disabled="formReadOnly"
                        data-vv-as="Category" 
                        :options="categories_arr" 
                        :multiple="false"
                        :hide-selected="true" 
                        placeholder="Select Category"
                        :class="{ 'is-invalid': errors.has('type_of_visit') }"
                        >
                        </multiselect>
                    </div>
                </div>
                <div class="col">
                    <div class="form-group">
                        <v-text
                        v-model="visit_report_data.visit_report_doctor.interacted_with"
                        :field_name="'visit_report_doctor[interacted_with]'"
                        :label="'Interacted With'"
                        :readonly="formReadOnly"
                        :rules="interactedWithRule"
                        :label="'Interacted With'"
                    ></v-text>
                    </div>
                </div>
                <div class="col">
                    <div class="form-group">
                        <v-number
                        v-model="visit_report_data.visit_report_doctor.number_of_visit_checklist"
                        :field_name="'visit_report_doctor[number_of_visit_checklist]'"
                        :label="'No. of visit checklist'"
                        :readonly="formReadOnly"
                        :rules="checklistRule"
                        :label="'Number of visit checklist'"
                    ></v-number>
                    </div>
                </div>
            </div>

            <div class="form-row">
                <div class="form-group col-sm-12">
                    <label class="required" for="">Inspectorate Team</label>            
                    <multiselect
                        v-validate="inspectorateTeamRule" 
                        :data-vv-name="'selected_visitors'"
                        :data-vv-value-path="selected_visitors"
                        :data-vv-as="'Inspectorate Team'" 
                        v-model="selected_visitors"
                        :disabled="formReadOnly"
                        :options="org_users_arr"
                        track-by="id"
                        label="name"
                        :multiple="true"
                        :hide-selected="true" 
                        placeholder="Select User"
                        @input="updateInspectorateTeamSelection"
                        :class="{ 'is-invalid': errors.has('selected_visitors') }"
                    >
                    </multiselect>
                    <div class="invalid-feedback">{{ errors.first('selected_visitors') }}</div>
                </div>
            </div>

            <div class="form-row">
                <div class="col">
                    <div class="form-group">

                        <v-date 
                            v-model="visit_report_data.visit_date" 
                            :field_name="'visit_report[visit_date]'" 
                            :rules="visitDateRule"
                            :label="'Visit Date'" 
                            :readonly="formReadOnly"
                        ></v-date>

                    </div>
                </div>

                <div class="col">
                    <div class="form-group">
                        
                        <v-time 
                            v-model="visit_report_data.visit_time_from"
                            :field_name="'visit_report[visit_time_from]'" 
                            :rules="visitTimeFromRule"
                            :label="'Visit Begin'"  
                            :readonly="formReadOnly"
                        ></v-time>
                
                    </div>
                </div>

                <div class="col">
                    <div class="form-group">

                        <v-time 
                            v-model="visit_report_data.visit_time_to" 
                            :field_name="'visit_report[visit_time_to]'"
                            :rules="visitTimeToRule" 
                            :label="'Visit End'" 
                            :readonly="formReadOnly"
                        ></v-time>
                        
                    </div>
                </div>

                <!-- hidden -->

                <input type="hidden" name="visit_report[visit_plan_id]" v-model="visit_report_data.visit_plan_id" value="">
                <input type="hidden" name="visit_report[visit_plan_item_id]" v-model="visit_report_data.visit_plan_item_id" value="">
                <input type="hidden" name="visit_report[visitable_type]" v-model="visit_report_data.visitable_type" value="">
                <input type="hidden" name="visit_report[visitable_id]" v-model="visit_report_data.visitable_id" value="">
                <input type="hidden" name="visit_report_doctor[doctor_id]" v-model="visit_report_data.visit_plan_item.visitable.id" value="">

                <select multiple name="visit_report_visitors[]" id="" class="hidden">
                    <option selected="selected" v-for="(visitor, index) in selected_visitors" :value="visitor.id">{{ visitor.name }}</option>
                </select>

                <input type="hidden" name="submit_action" v-model="submit_action" />
                
            </div>

