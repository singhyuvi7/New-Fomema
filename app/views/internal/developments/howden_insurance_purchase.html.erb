<form method="POST" action="<%= @request_data["RESPONSE_URL"] %>">
    <div class="card">
        <div class="card-header">
            HOWDEN
        </div>
        <div class="card-body">
            <p><%= @request_data["RESPONSE_URL"] %></p>
            <textarea name="param" style="width: 100%; height: 100px"><%= InsuranceService.encrypt(@response_data.to_json) %></textarea>
            <textarea style="width: 100%; height: 500px"><%= JSON.pretty_generate(@response_data) %></textarea>
        </div>
        <div class="card-footer text-right">
            <button type="submit" id="submit" class="btn btn-primary">Return</button>
        </div>
    </div>
</form>
