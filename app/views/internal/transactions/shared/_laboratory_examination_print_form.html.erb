                        <label class="form-check-label" for="<%= input_key %>_tpha_reactive">
                            REACTIVE
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="tpha_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][tpha]" id="<%= input_key %>_tpha_negative" value="NEGATIVE"
                            <% if !laboratory_examination_model.tpha %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_tpha_negative">
                            NEGATIVE
                        </label>
                    </div>
                </td>
            </tr>
            <!--End of tpha -->

            <!-- Start of malaria -->
            <tr>
                <td>6</td>
                <td colspan="4">Malaria (Screening)</td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="malaria_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][malaria]" id="<%= input_key %>_malaria_positive" value="POSITIVE"
                            <% if laboratory_examination_model.malaria %> checked <% end %> required>
                        <label class="form-check-label" for="<%= input_key %>_malaria_positive">
                            POSITIVE
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="malaria_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][malaria]" id="<%= input_key %>_malaria_negative" value="NEGATIVE"
                            <% if !laboratory_examination_model.malaria %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_malaria_negative">
                            NEGATIVE
                        </label>
                    </div>
                </td>
            </tr>
            <!--End of malaria -->

            <!-- Start of BFMP -->
            <tr>
                <td>7</td>
                <td colspan="4">BFMP</td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="bfmp_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][bfmp]" id="<%= input_key %>_bfmp_reactive" value="REACTIVE"
                            <% if laboratory_examination_model.bfmp %> checked <% end %> required>
                        <label class="form-check-label" for="<%= input_key %>_bfmp_reactive">
                            REACTIVE
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="bfmp_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][bfmp]" id="<%= input_key %>_bfmp_negative" value="NEGATIVE"
                            <% if !laboratory_examination_model.bfmp %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_bfmp_negative">
