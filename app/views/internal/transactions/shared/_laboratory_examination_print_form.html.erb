                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="sugar_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][sugar]" id="<%= input_key %>_sugar_absent" value="ABSENT"
                            <% if !laboratory_examination_model.sugar %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_sugar_absent">
