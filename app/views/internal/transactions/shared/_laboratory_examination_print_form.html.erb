                    </div>
                </td>
            </tr>
            <!--End of BFMP -->

            <!-- Start of urine -->
            <tr>
                <td colspan="7">Urine Tests</td>
            </tr>
            <!--End of urine -->

            <!-- Start of opiates -->
            <tr>
                <td>8</td>
                <td colspan="4">Opiates</td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="opiates_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][opiates]" id="<%= input_key %>_opiates_reactive" value="REACTIVE"
                            <% if laboratory_examination_model.opiates %> checked <% end %> required>
                        <label class="form-check-label" for="<%= input_key %>_opiates_reactive">
                            REACTIVE
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="opiates_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][opiates]" id="<%= input_key %>_opiates_negative" value="NEGATIVE"
                            <% if !laboratory_examination_model.opiates %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_opiates_negative">
                            NEGATIVE
                        </label>
                    </div>
                </td>
            </tr>
            <!--End of opiates -->

            <!-- Start of cannabis -->
            <tr>
                <td>9</td>
                <td colspan="4">Cannabis</td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="cannabis_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][cannabis]" id="<%= input_key %>_cannabis_reactive" value="REACTIVE"
