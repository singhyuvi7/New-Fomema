                            name="<%= input_key %>[laboratory_examination][vdrl]" id="<%= input_key %>_vdrl_reactive" value="REACTIVE"
                            <% if laboratory_examination_model.vdrl %> checked <% end %> required>
                        <label class="form-check-label" for="<%= input_key %>_vdrl_reactive">
                            REACTIVE
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="vdrl_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][vdrl]" id="<%= input_key %>_vdrl_negative" value="NEGATIVE"
                            <% if !laboratory_examination_model.vdrl %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_vdrl_negative">
                            NEGATIVE
                        </label>
                    </div>
                </td>
            </tr>
            <!--End of syphilis -->

            <!-- Start of tpha -->
            <tr>
                <td>5</td>
                <td colspan="4">TPHA/TPPA</td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="tpha_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][tpha]" id="<%= input_key %>_tpha_reactive" value="REACTIVE"
