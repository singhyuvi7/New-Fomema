                            <% if !laboratory_examination_model.elisa %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_elisa_negative">
                            NEGATIVE
                        </label>
                    </div>
                </td>
            </tr>
            <!--End of HIV -->

            <!-- Start of HBSAG -->
            <tr>
                <td>3</td>
                <td colspan="4">HBsAg</td>
                <td width="110" class="text-center">
