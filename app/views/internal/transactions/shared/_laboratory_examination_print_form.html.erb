<div class="card xray_report_form">
    <table class="table table-bordered table-card">
        <thead>
            <tr>
                <th colspan="5">Report</th>
                <th colspan="2" class="text-center">Status</th>
            </tr>
        </thead>
        <tbody>
            <!-- Start of Laboratory test not done -->
            <tr>
                <td colspan="5">Laboratory tests not done</td>

                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="laboratory_test_not_done_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][laboratory_test_not_done]" id="<%= input_key %>_laboratory_test_not_done_yes" value="YES"
                            <% if laboratory_examination_model.laboratory_test_not_done === 'YES' %> checked <% end %> required>
                        <label class="form-check-label" for="<%= input_key %>_laboratory_test_not_done_yes">
                            YES
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="laboratory_test_not_done_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][laboratory_test_not_done]" id="<%= input_key %>_laboratory_test_not_done_no" value="NO"
                            <% if laboratory_examination_model.laboratory_test_not_done === 'NO' %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_laboratory_test_not_done_no">
                            NO
                        </label>
                    </div>
                </td>
            </tr>
            <!--End of Laboratory test not done -->

            <!-- Start of date and time of specimen taken -->
            <tr>
                <td colspan="5">Date & time of specimen taken</td>

                <td colspan="2">
                    <textarea disabled class="form-control" name="<%= input_key %>[laboratory_examination][specimen_taken_date]"
                        id="<%= input_key %>_laboratory_examination_specimen_taken_date" rows="2"><%= laboratory_examination_model.specimen_taken_date %></textarea>
                </td>
            </tr>
            <!--End of date and time of specimen taken -->

            <!-- Start of date and time of specimen received -->
            <tr>
                <td colspan="5">Date & time of specimen received</td>

                <td colspan="2">
                    <textarea disabled class="form-control" name="<%= input_key %>[laboratory_examination][specimen_received_date]"
                        id="<%= input_key %>_laboratory_examination_specimen_received_date" rows="2"><%= laboratory_examination_model.specimen_received_date %></textarea>
                </td>
            </tr>
            <!--End of date and time of specimen received -->

            <!-- Start of date and time of specimen received -->
            <tr>
                <td colspan="5">Barcode of blood specimen</td>

                <td colspan="2">
                    <textarea disabled class="form-control" name="<%= input_key %>[laboratory_examination][blood_specimen_barcode]"
                        id="<%= input_key %>_laboratory_examination_blood_specimen_barcode" rows="2"><%= laboratory_examination_model.blood_specimen_barcode %></textarea>
                </td>
            </tr>
            <!--End of date and time of specimen received -->

            <!-- Start of date and time of specimen received -->
            <tr>
                <td colspan="5">Barcode of urine specimen</td>

                <td colspan="2">
                    <textarea disabled class="form-control" name="<%= input_key %>[laboratory_examination][urine_specimen_barcode]"
                        id="<%= input_key %>_laboratory_examination_urine_specimen_barcode" rows="2"><%= laboratory_examination_model.urine_specimen_barcode %></textarea>
                </td>
            </tr>
            <!--End of date and time of specimen received -->

            <!-- Start of blood -->
            <tr>
                <td colspan="7">Blood Tests</td>
            </tr>
            <!--End of blood -->

            <tr>
                <th width="50">No</th>
                <th colspan="4">Report</th>
                <th colspan="2" class="text-center">Status</th>
            </tr>

            <!-- Start of blood group -->
            <tr>
                <td>1</td>
                <td>Blood Group Type</td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="blood_group_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][blood_group]" id="<%= input_key %>_blood_group_a" value="A"
                            <% if laboratory_examination_model.blood_group === 'A' %> checked <% end %> required>
                        <label class="form-check-label" for="<%= input_key %>_blood_group_a">
                            A
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="blood_group_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][blood_group]" id="<%= input_key %>_blood_group_b" value="B"
                            <% if laboratory_examination_model.blood_group === 'B' %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_blood_group_b">
                            B
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="blood_group_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][blood_group]" id="<%= input_key %>_blood_group_ab" value="AB"
                            <% if laboratory_examination_model.blood_group === 'AB' %> checked <% end %> required>
                        <label class="form-check-label" for="<%= input_key %>_blood_group_ab">
                            AB
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="blood_group_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][blood_group]" id="<%= input_key %>_blood_group_o" value="O"
                            <% if laboratory_examination_model.blood_group === 'O' %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_blood_group_o">
                            O
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="blood_group_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][blood_group]" id="<%= input_key %>_blood_group_other" value="OTHER"
                            <% if laboratory_examination_model.blood_group === 'OTHER' %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_blood_group_other">
                            OTHER
                        </label>
                    </div>
                </td>
            </tr>
            <!--End of blood group -->

            <!-- Start of blood group is other type -->
            <tr>
                <td></td>
                <td colspan="4">If Blood Group is Other type</td>

                <td colspan="2">
                    <textarea disabled class="form-control" name="<%= input_key %>[laboratory_examination][blood_group_other]"
                        id="<%= input_key %>_laboratory_examination_blood_group_other" rows="2"><%= laboratory_examination_model.blood_group_other %></textarea>
                </td>
            </tr>
            <!-- End of blood group is other type -->

            <!-- Start of blood group rh -->
            <tr>
                <td></td>
                <td colspan="3">Blood Group RH</td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="blood_group_rhesus_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][blood_group_rhesus]" id="<%= input_key %>_blood_group_rhesus_positive" value="+VE"
                            <% if laboratory_examination_model.blood_group_rhesus === '+VE' %> checked <% end %> required>
                        <label class="form-check-label" for="<%= input_key %>_blood_group_rhesus_positive">
                            +VE
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="blood_group_rhesus_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][blood_group_rhesus]" id="<%= input_key %>_blood_group_rhesus_negative" value="-VE"
                            <% if laboratory_examination_model.blood_group_rhesus === '-VE' %> checked <% end %>>
                        <label class="form-check-label" for="<%= input_key %>_blood_group_rhesus_negative">
                            -VE
                        </label>
                    </div>
                </td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="blood_group_rhesus_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][blood_group_rhesus]" id="<%= input_key %>_blood_group_rhesus_other" value="OTHER"
                            <% if laboratory_examination_model.blood_group_rhesus === 'OTHER' %> checked <% end %> required>
                        <label class="form-check-label" for="<%= input_key %>_blood_group_rhesus_other">
                            OTHER
                        </label>
                    </div>
                </td>
            </tr>
            <!--End of blood group rh -->

            <!-- Start of blood group rh is other type -->
            <tr>
                <td></td>
                <td colspan="4">If Blood Group RH is Other type</td>

                <td colspan="2">
                    <textarea disabled class="form-control" name="<%= input_key %>[laboratory_examination][blood_group_rhesus_other]"
                        id="<%= input_key %>_laboratory_examination_blood_group_rhesus_other" rows="2"><%= laboratory_examination_model.blood_group_rhesus_other %></textarea>
                </td>
            </tr>
            <!-- End of blood group rh is other type -->

            <!-- Start of HIV -->
            <tr>
                <td>2</td>
                <td colspan="4">HIV</td>
                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="elisa_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][elisa]" id="<%= input_key %>_elisa_reactive" value="REACTIVE"
