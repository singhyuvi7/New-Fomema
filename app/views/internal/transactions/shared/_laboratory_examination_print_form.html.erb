                <td width="110" class="text-center">
                    <div class="form-check form-check-inline">
                        <input disabled class="pregnancy_status form-check-input" type="radio"
                            name="<%= input_key %>[laboratory_examination][pregnancy]" id="<%= input_key %>_pregnancy_negative" value="NEGATIVE"
                            <% if !laboratory_examination_model.pregnancy %> checked <% end %>>
