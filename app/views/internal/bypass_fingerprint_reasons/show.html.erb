<div class="card">

    <div class="card-header">
        <strong>Bypass Fingerprint Reasons</strong> - Bypass Fingerprint Reason Detail
    </div>

    <div class="card-body">

        <%=render "/shared/all_flash" %>

        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="bypass_fingerprint_reason_code">Code</label>
                    <p class="form-control-plaintext">
                        <%= @bypass_fingerprint_reason.code %>
                    </p>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="bypass_fingerprint_reason_code">Description</label>
                    <p class="form-control-plaintext">
                        <%= @bypass_fingerprint_reason.description %>
                    </p>
                </div>
            </div>
        </div>

    </div>

    <div class="card-footer">

        <div class="text-right">
            <%=raw show_back_button(internal_bypass_fingerprint_reasons_path) %>
            <% if has_permission?('EDIT_BYPASS_FINGERPRINT_REASON') %>
                <%=raw show_edit_button(edit_internal_bypass_fingerprint_reason_path(@bypass_fingerprint_reason)) %>
            <% end %>
        </div>

    </div>

</div>
<!-- /.card -->

<div class="card">
    <div class="card-body">
        <ul class="list-group">
            <% if has_permission?("VIEW_AUDIT_BYPASS_FINGERPRINT_REASON") %>
                <li class="list-group-item">
                    <%= link_to "Audits", internal_bypass_fingerprint_reason_audits_path(@bypass_fingerprint_reason) %>
                </li>
            <% end %>
        </ul>
    </div>
</div>