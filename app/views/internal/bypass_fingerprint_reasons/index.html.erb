<div class="card">
    <div class="card-header">
        <strong>Bypass Fingerprint Reasons</strong>
    </div>

    <!-- card-body -->
    <div class="card-body">
        <%=render "filter" %>

        <%=render "/shared/all_flash" %>

        <div class="row">
            <div class="col module-actions text-right">
                <% if has_permission?('CREATE_BYPASS_FINGERPRINT_REASON') %>
                    <%=raw index_table_new_button(new_internal_bypass_fingerprint_reason_path) %>
                <% end %>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="table-responsive">
<table class="<%= index_table_class %>">
    <thead>
        <tr>
            <th class="module-row-numbers">#</th>
            <th>Code</th>
            <th>Description</th>
            <th class="module-row-actions">Action(s)</th>
        </tr>
    </thead>

    <tbody>
        <% if @bypass_fingerprint_reasons.blank? %>
            <tr>
                <td colspan="4" class="text-danger">No records found.</td>
            </tr>
        <% else %>

            <% @bypass_fingerprint_reasons.each.with_index((@bypass_fingerprint_reasons.current_page - 1) * get_per + 1) do |bypass_fingerprint_reason, index| %>
                <tr>
                    <td class="module-row-numbers"><%= index %></td>
                    <td><%= bypass_fingerprint_reason.code %></td>
                    <td><%= bypass_fingerprint_reason.description %></td>
                    <td class="module-row-actions">
                        <%=raw index_table_show_button(internal_bypass_fingerprint_reason_path(bypass_fingerprint_reason)) %>
                        <% if has_permission?('EDIT_BYPASS_FINGERPRINT_REASON') %>
                            <%=raw index_table_edit_button edit_internal_bypass_fingerprint_reason_path(bypass_fingerprint_reason) %>
                        <% end %>
                        <% if has_permission?('DELETE_BYPASS_FINGERPRINT_REASON') %>
                            <%=raw index_table_delete_button(internal_bypass_fingerprint_reason_path(bypass_fingerprint_reason)) %>
                        <% end %>
                    </td>
                </tr>
            <% end %>

        <% end %>
    </tbody>
</table>
                </div>
            </div>
        </div>
        <div class="row">
            <%= paginate @bypass_fingerprint_reasons %>
        </div>
    </div>
    <!-- /card-body -->
</div>