<div class="card">

    <div class="card-header">
        <strong>Finance Setting</strong> - Detail
    </div>

    <div class="card-body">

        <%=render '/shared/all_flash' %>

        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="category">Category</label>
                    <p class="form-control-plaintext">
                        <%= @finance_setting.category %>
                    </p>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="code">Code</label>
                    <p class="form-control-plaintext">
                        <%= @finance_setting.code %>
                    </p>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="description">Description</label>
                    <p class="form-control-plaintext">
                        <%= @finance_setting.description %>
                    </p>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="value">Value</label>
                    <p class="form-control-plaintext">
                        <%= @finance_setting.value %>
                    </p>
                </div>
            </div>
        </div>

    </div>

    <div class="card-footer">

        <div class="text-right">
            <%=raw show_back_button(internal_finance_settings_path) %>
            <% if has_permission?('EDIT_FINANCE_SETTING') %>
            <%=raw show_edit_button(internal_finance_setting_path(@finance_setting)) %>
            <% end %>
        </div>

    </div>

</div>
<!-- /.card -->

<div class="card">
    <div class="card-body">
        <ul class="list-group">
            <% if has_permission?("VIEW_AUDIT_FINANCE_SETTING") %>
            <li class="list-group-item">
                <%= link_to "Audits", internal_finance_setting_audits_path(@finance_setting) %>
            </li>
            <% end %>
        </ul>
    </div>
</div>