<div class="card">

    <div class="card-header">
        <strong>Fees</strong> - Fee Detail
    </div>

    <div class="card-body">

        <%=render '/shared/all_flash' %>

        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="fee_code">Code</label>
                    <p class="form-control-plaintext">
                        <%= @fee.code %>
                    </p>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="fee_name">Name</label>
                    <p class="form-control-plaintext">
                        <%= @fee.name %>
                    </p>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="fee_remark">Remark</label>
                    <p class="form-control-plaintext">
                        <%= @fee.remark %>
                    </p>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="fee_amount">Amount</label>
                    <p class="form-control-plaintext">
                        <%= @fee.amount %>
                    </p>
                </div>
            </div>
        </div>

    </div>

    <div class="card-footer">

        <div class="text-right">
            <%=raw show_back_button(internal_fees_path) %>
            <% if has_permission?('EDIT_FEE') %>
                <%=raw show_edit_button(edit_internal_fee_path(@fee)) %>
            <% end %>
        </div>

    </div>

</div>
<!-- /.card -->

<div class="card">
    <div class="card-body">
        <ul class="list-group">
            <% if has_permission?("VIEW_AUDIT_FEE") %>
                <li class="list-group-item">
                    <%= link_to "Audits", internal_fee_audits_path(@fee) %>
                </li>
            <% end %>
        </ul>
    </div>
</div>