                                        <v-textarea v-model="unacceptable_fields.foreign_worker.without_original_passport_comment" :field_name="'unacceptable_fields[foreign_worker][without_original_passport_comment]'" :id="'unacceptable_fields[foreign_worker][without_original_passport_comment]'" :rules="{'required':(unacceptable_fields.foreign_worker.without_original_passport)}" :label="''" :validation_label="'Comment'" :readonly="formReadOnly"></v-textarea>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col-sm-6 col-md-6">
                                        <input type="checkbox" name="unacceptable_fields[foreign_worker][using_photocopy_passport]" v-model="unacceptable_fields.foreign_worker.using_photocopy_passport" value="1" v-validate="foreignWorkerUnacceptableRule" data-vv-as="'Unacceptable(s)/Others'" :disabled="formDisabled" data-vv-as="Verification using photocopy passport" id="unacceptable_fields[foreign_worker][using_photocopy_passport]">
                                        <input class="form-control" type="hidden" :class="{ 'is-invalid': errors.has('unacceptable_fields[foreign_worker][using_photocopy_passport]') }" />
                                        <label class="form-check-label" for="unacceptable_fields[foreign_worker][using_photocopy_passport]">Verification using photocopy passport</label>
                                        <div class="invalid-feedback">{{ errors.first('unacceptable_fields[foreign_worker][using_photocopy_passport]') }}</div>
                                    </div>
                                    <div class="form-group col-sm-6 col-md-6">
                                        <v-textarea v-model="unacceptable_fields.foreign_worker.using_photocopy_passport_comment" :field_name="'unacceptable_fields[foreign_worker][using_photocopy_passport_comment]'" :id="'unacceptable_fields[foreign_worker][using_photocopy_passport_comment]'" :rules="{'required':(unacceptable_fields.foreign_worker.using_photocopy_passport)}" :label="''" :validation_label="'Comment'" :readonly="formReadOnly"></v-textarea>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col-sm-6 col-md-6">
                                        <input type="checkbox" name="unacceptable_fields[foreign_worker][using_id_card]" v-model="unacceptable_fields.foreign_worker.using_id_card" value="1" v-validate="foreignWorkerUnacceptableRule" data-vv-as="'Unacceptable(s)/Others'" :disabled="formDisabled" data-vv-as="Verification using ID card/name tag" id="unacceptable_fields[foreign_worker][using_id_card]">
                                        <input class="form-control" type="hidden" :class="{ 'is-invalid': errors.has('unacceptable_fields[foreign_worker][using_id_card]') }" />
                                        <label class="form-check-label" for="unacceptable_fields[foreign_worker][using_id_card]">Verification using ID card/name tag</label>
                                        <div class="invalid-feedback">{{ errors.first('unacceptable_fields[foreign_worker][using_id_card]') }}</div>
                                    </div>
                                    <div class="form-group col-sm-6 col-md-6">
                                        <v-textarea v-model="unacceptable_fields.foreign_worker.using_id_card_comment" :field_name="'unacceptable_fields[foreign_worker][using_id_card_comment]'" :id="'unacceptable_fields[foreign_worker][using_id_card_comment]'" :rules="{'required':(unacceptable_fields.foreign_worker.using_id_card)}" :label="''" :validation_label="'Comment'" :readonly="formReadOnly"></v-textarea>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col-sm-6 col-md-6">
                                        <input type="checkbox" name="unacceptable_fields[foreign_worker][using_fake_passport]" v-model="unacceptable_fields.foreign_worker.using_fake_passport" value="1" v-validate="foreignWorkerUnacceptableRule" data-vv-as="'Unacceptable(s)/Others'" :disabled="formDisabled" data-vv-as="Verification using fake passport" id="unacceptable_fields[foreign_worker][using_fake_passport]">
                                        <input class="form-control" type="hidden" :class="{ 'is-invalid': errors.has('unacceptable_fields[foreign_worker][using_fake_passport]') }" />
                                        <label class="form-check-label" for="unacceptable_fields[foreign_worker][using_fake_passport]">Verification using fake passport</label>
                                        <div class="invalid-feedback">{{ errors.first('unacceptable_fields[foreign_worker][using_fake_passport]') }}</div>
                                    </div>
                                    <div class="form-group col-sm-6 col-md-6">
                                        <v-textarea v-model="unacceptable_fields.foreign_worker.using_fake_passport_comment" :field_name="'unacceptable_fields[foreign_worker][using_fake_passport_comment]'" :id="'unacceptable_fields[foreign_worker][using_fake_passport_comment]'" :rules="{'required':(unacceptable_fields.foreign_worker.using_fake_passport)}" :label="''" :validation_label="'Comment'" :readonly="formReadOnly"></v-textarea>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col-sm-6 col-md-6">
                                        <input type="checkbox" name="unacceptable_fields[foreign_worker][others]" v-model="unacceptable_fields.foreign_worker.others" value="1" v-validate="foreignWorkerUnacceptableRule" data-vv-as="'Unacceptable(s)/Others'" :disabled="formDisabled" data-vv-as="Others" id="unacceptable_fields[foreign_worker][others]">
                                        <input class="form-control" type="hidden" :class="{ 'is-invalid': errors.has('unacceptable_fields[foreign_worker][others]') }" />
                                        <label class="form-check-label" for="unacceptable_fields[foreign_worker][others]">Others</label>
                                        <div class="invalid-feedback">{{ errors.first('unacceptable_fields[foreign_worker][others]') }}</div>
                                    </div>
                                    <div class="form-group col-sm-6 col-md-6">
                                        <v-textarea v-model="unacceptable_fields.foreign_worker.others_comment" :field_name="'unacceptable_fields[foreign_worker][others_comment]'" :id="'unacceptable_fields[foreign_worker][others_comment]'" :rules="{'required':(unacceptable_fields.foreign_worker.others)}" :label="''" :validation_label="'Comment'" :readonly="formReadOnly"></v-textarea>
                                    </div>
                                </div>
                            </div>
                        </td>
                        <td class="text-center">
                            <div class="form-check form-check-inline">

                                <v-radio v-model="visit_report_data.visit_report_xray_facility.foreign_worker_present_acceptable" :radio_value="'Y'" :field_name="'visit_report_xray_facility[foreign_worker_present_acceptable]'" :rules="foreignWorkerPresentAcceptableRule" :label="'Yes'" :validation_label="'Acceptable'" :disabled="formDisabled" :id="'visit_report_xray_facility[foreign_worker_present_acceptable][yes]'"></v-radio>

                            </div>

                            <div class="invalid-feedback">{{ errors.first('visit_report_xray_facility[foreign_worker_present_acceptable]') }}</div>
                        </td>
                        <td class="text-center">
                            <div class="form-check form-check-inline">

                                <v-radio v-model="visit_report_data.visit_report_xray_facility.foreign_worker_present_acceptable" :radio_value="'N'" :field_name="'visit_report_xray_facility[foreign_worker_present_acceptable]'" :rules="foreignWorkerPresentAcceptableRule" :label="'No'" :validation_label="'Acceptable'" :disabled="formDisabled" :id="'visit_report_xray_facility[foreign_worker_present_acceptable][no]'"></v-radio>


                            </div>
                        </td>
                        <td class="text-center">
                            <div class="form-check form-check-inline">

                                <v-radio v-model="visit_report_data.visit_report_xray_facility.foreign_worker_present_acceptable" :radio_value="'NA'" :field_name="'visit_report_xray_facility[foreign_worker_present_acceptable]'" :rules="foreignWorkerPresentAcceptableRule" :label="'N/A'" :validation_label="'Acceptable'" :disabled="formDisabled" :id="'visit_report_xray_facility[foreign_worker_present_acceptable][na]'"></v-radio>


                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td rowspan="2">2.</td>
                        <td colspan="2">
                            Valid annual practicing certificate(APC)
                            <div class="form-group">
                                <div class="row">
                                    <div class="form-group col-sm-6">
                                        <v-text v-model="visit_report_data.visit_report_xray_facility.apc_number" :field_name="'visit_report_xray_facility[apc_number]'" :rules="apcNoRule" :label="'APC No.'" :validation_label="'APC No.'" :readonly="formReadOnly"></v-text>
                                    </div>
                                    <div class="form-group col-sm-6" v-if="formReadOnly">
                                        <v-text v-model="visit_report_data.visit_report_xray_facility.apc_year" :field_name="'visit_report_xray_facility[apc_year]'" :label="'Year'" :rules="apcYearRule" :validation_label="'APC Year'" :readonly="formReadOnly"></v-text>
                                    </div>
                                    <div class="form-group col-sm-6" v-if="!formReadOnly">
                                        <label for="visit_report_xray_facility[apc_year]" class="required">Year</label>
                                        <select class="form-control" v-validate="apcYearRule" name="visit_report_xray_facility[apc_year]" data-vv-as="APC Year" v-model="visit_report_data.visit_report_xray_facility.apc_year" id="apc_year" :class="{ 'is-invalid': errors.has('visit_report_xray_facility[apc_year]') }" :disabled="formReadOnly">
                                            <option disabled value="">Select option</option>
                                            <option v-for="year in years" :value="year">{{ year }}</option>
                                        </select>
                                        <div class="invalid-feedback">{{ errors.first('visit_report_xray_facility[apc_year]') }}</div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label>If Unacceptable, Please Specify :</label>
                                <div class="form-row">
                                    <div class="form-group col-sm-6 col-md-6">
                                        <input type="checkbox" name="unacceptable_fields[apc][not_available_current_year]" v-model="unacceptable_fields.apc.not_available_current_year" value="1" v-validate="apcUnacceptableRule" data-vv-as="'Unacceptable(s)/Others'" :disabled="formDisabled" data-vv-as="APC not available for current year" id="unacceptable_fields[apc][not_available_current_year]">
                                        <input class="form-control" type="hidden" :class="{ 'is-invalid': errors.has('unacceptable_fields[apc][not_available_current_year]') }" />
                                        <label class="form-check-label" for="unacceptable_fields[apc][not_available_current_year]">APC not available for current year</label>
