                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col-sm-6 col-md-6">
                                        <input type="checkbox" name="unacceptable_fields[foreign_worker][without_original_passport]" v-model="unacceptable_fields.foreign_worker.without_original_passport" value="1" v-validate="foreignWorkerUnacceptableRule" data-vv-as="'Unacceptable(s)/Others'" :disabled="formDisabled" data-vv-as="Verification without using original passport" id="unacceptable_fields[foreign_worker][without_original_passport]">
                                        <input class="form-control" type="hidden" :class="{ 'is-invalid': errors.has('unacceptable_fields[foreign_worker][without_original_passport]') }" />
                                        <label class="form-check-label" for="unacceptable_fields[foreign_worker][without_original_passport]">Verification without using original passport</label>
