<div class="row">

    <div class="col-md-12">

        <%= render "/shared/all_flash" %>

        <div v-if="visit_report_data.status" class="alert alert-info" role="alert">
            LATEST STATUS: <b><%= VisitReport::STATUSES[@visit_report.status] %></b>
        </div>

        <div v-if="visit_report_data.status === 'REJECTED'" class="alert alert-danger" role="alert">
            COMMENT ON REJECTION: <b v-if="visit_report_data.level_1_approval_decision === 'REJECT'">{{ visit_report_data.level_1_approval_comment }}</b><b v-if="visit_report_data.level_2_approval_decision === 'REJECT'">{{ visit_report_data.level_2_approval_comment }}</b>
        </div>

        <div v-if="!canEdit" class="alert alert-warning" role="alert">
            FORM UPDATE IS DISABLE FOR STATUS: <b><%= VisitReport::STATUSES[@visit_report.status] %></b>
        </div>

        <form ref="visit_report_form" method="POST" action="<%= form_action %>">

            <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

            <% if form_method.downcase !='post' %>
            <input type="hidden" name="_method" value="<%= form_method %>">
            <% end %>

            <div class="form-row">
                <div class="col">
                    <div class="form-group">

                        <v-text v-model="visit_report_data.code" :field_name="'visit_report[code]'" :label="'Report ID'" :readonly="true"></v-text>

                    </div>
                </div>
                <div class="col">
                    <div class="form-group">
                        <v-text v-model="visit_report_data.visit_plan_item.visitable.code" :field_name="'visit_report_xray_facility[code]'" :label="'X-Ray Code'" :readonly="true"></v-text>
                    </div>
                </div>
            </div>

            <div class="form-row">
                <div class="col">
                    <div class="form-group">
                        <v-text v-model="visit_report_data.visit_plan_item.visitable.license_holder_name" :field_name="'visit_report_xray_facility[license_holder_name]'" :label="'License Holder Name'" :readonly="true"></v-text>
                    </div>
                </div>
                <div class="col">
                    <div class="form-group">
                        <label>X-Ray Facility Address</label>
                        <textarea class="form-control" rows="1" id="visit_report_data.xray_address" :readonly="true">{{ xray_address.filter(function(address) { return address; }).join(',') }}
                        </textarea>
                    </div>
                </div>
            </div>

            <div class="form-row">
                <div class="col">
                    <div class="form-group">
                        <label class="required" for="visit_report_xray_facility[type_of_visit]">Type Of Visit</label>
                        <select class="form-control" v-validate="categoryRule" name="visit_report_xray_facility[type_of_visit]" v-model="visit_report_data.visit_report_xray_facility.type_of_visit" id="type_of_visit" :class="{ 'is-invalid': errors.has('visit_report_xray_facility[type_of_visit]') }" :disabled="formReadOnly" data-vv-as="Type of visit">
                            <option disabled value="">Select option</option>
                            <option v-for="(category, index) in categories_arr" :value="category">{{ category }}
                            </option>
                        </select>
                        <div class="invalid-feedback">{{ errors.first('visit_report_xray_facility[type_of_visit]') }}
                        </div>
                    </div>
                </div>

                <div class="col">
                    <div class="form-group">
                        <v-text v-model="visit_report_data.visit_report_xray_facility.number_of_visit_checklist" :field_name="'visit_report_xray_facility[number_of_visit_checklist]'" :readonly="formReadOnly" :rules="checklistRule" :label="'Number Of Visit Checklist'" :validation_label="'Number Of Visit Checklist'"></v-text>
                    </div>
                </div>
            </div>

            <div class="form-row">
                <div class="form-group col-sm-12">
                    <label class="required" for="">Inspectorate Team</label>
                    <multiselect v-validate="inspectorateTeamRule" :data-vv-name="'selected_visitors'" :data-vv-value-path="selected_visitors" :data-vv-as="'Inspectorate Team'" v-model="selected_visitors" :disabled="formReadOnly" :options="org_users_arr" track-by="id" label="name" :multiple="true" :hide-selected="true" placeholder="Select user or type to add non-fomema officer" tag-placeholder="Please enter to add non-fomema officer" :class="{ 'is-invalid': errors.has('selected_visitors') }" :taggable="true" @tag="addTag">
                    </multiselect>
                    <div class="invalid-feedback">{{ errors.first('selected_visitors') }}</div>
                </div>
            </div>

            <div class="form-row">
                <div class="col">
                    <div class="form-group">
                        <v-text v-model="visit_report_data.visit_report_xray_facility.interacted_with" :field_name="'visit_report_xray_facility[interacted_with]'" :readonly="formReadOnly" :rules="interactedWithRule" :label="'Interacted With'" :validation_label="'Interacted With'"></v-text>
                    </div>
                </div>
                <div class="col">
                    <div class="form-group">
                        <v-text v-model="visit_report_data.visit_report_xray_facility.radiographer_name" :field_name="'visit_report_xray_facility[radiographer_name]'" :readonly="formReadOnly" :rules="radiographerNameRule" :label="'Radiographer Name'" :validation_label="'Radiographer Name'"></v-text>
                    </div>
                </div>
            </div>

            <div class="form-row">
                <div class="col">
                    <div class="form-group">

                        <v-date v-model="visit_report_data.visit_date" :field_name="'visit_report[visit_date]'" :rules="visitDateRule" :label="'Visit Date'" :readonly="formReadOnly" id="visit_date"  :max_date="visitDateMax"></v-date>

                    </div>
                </div>

                <div class="col">
                    <div class="form-group">

                        <v-time v-model="visit_report_data.visit_time_from" :field_name="'visit_report[visit_time_from]'" :rules="visitTimeFromRule" :label="'Visit Begin'" :readonly="formReadOnly" id="visit_time_from"></v-time>

                    </div>
                </div>

                <div class="col">
                    <div class="form-group">

                        <v-time v-model="visit_report_data.visit_time_to" :field_name="'visit_report[visit_time_to]'" :rules="visitTimeToRule" :label="'Visit End'" :readonly="formReadOnly" id="visit_time_to"></v-time>

                    </div>
                </div>
            </div>

            <div class="form-row">
                <div class="col">
                    <div class="form-group">
                        <v-text v-model="last_visited_date" :field_name="'last_visited_date'" :label="'Last Visit Date'" :readonly="readOnlyField" id="last_visited_date"></v-text>
                    </div>
                </div>
                <div class="col">
                    <div class="form-group">
                        <v-text v-model="allocations_last_current_year" :field_name="'year_allocation'" :label="'Allocation '+last_year+'/'+current_year" :readonly="readOnlyField" id="year_allocation"></v-text>
                    </div>
                </div>
            </div>

            <h4>Summary Of Findings</h4>

            <table class="table table-bordered table-sm table-hover">
                <thead>
                    <tr>
                        <th></th>
                        <th colspan="2"></th>
                        <th colspan="3" class="text-center">Acceptable</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>1.</td>
                        <td colspan="2">
                            Verification Of Identity Of Foreign Worker.

                            <div class="form-group">

                                <v-text v-model="visit_report_data.visit_report_xray_facility.foreign_worker_present" :field_name="'visit_report_xray_facility[foreign_worker_present]'" :rules="foreignWorkerPresentRule" :label="'No Of Foreign Workers Present'" :validation_label="'No Of Foreign Workers Present'" :readonly="formReadOnly"></v-text>

                            </div>

                            <div class="form-group">
                                <label>If Unacceptable, Please Specify :</label>
                                <div class="form-row">
                                    <div class="form-group col-sm-6 col-md-6">
                                        <input type="checkbox" name="unacceptable_fields[foreign_worker][no_xray_examination]" v-model="unacceptable_fields.foreign_worker.no_xray_examination" value="1" v-validate="foreignWorkerUnacceptableRule" data-vv-as="'Unacceptable(s)/Others'" :disabled="formDisabled" data-vv-as="No verification of identity prior to x-ray examination" id="unacceptable_fields[foreign_worker][no_xray_examination]">
                                        <input class="form-control" type="hidden" :class="{ 'is-invalid': errors.has('unacceptable_fields[foreign_worker][no_xray_examination]') }" />
                                        <label class="form-check-label" for="unacceptable_fields[foreign_worker][no_xray_examination]">No verification of identity prior to x-ray examination</label>
                                        <div class="invalid-feedback">
                                            {{ errors.first('unacceptable_fields[foreign_worker][no_xray_examination]') }}
                                        </div>
                                    </div>
                                    <div class="form-group col-sm-6 col-md-6">
                                        <v-textarea v-model="unacceptable_fields.foreign_worker.no_xray_examination_comment" :field_name="'unacceptable_fields[foreign_worker][no_xray_examination_comment]'" :id="'unacceptable_fields[foreign_worker][no_xray_examination_comment]'" :rules="{'required':(unacceptable_fields.foreign_worker.no_xray_examination)}" :label="''" :validation_label="'Comment'" :readonly="formReadOnly"></v-textarea>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col-sm-6 col-md-6">
                                        <input type="checkbox" name="unacceptable_fields[foreign_worker][without_biometric_device]" v-model="unacceptable_fields.foreign_worker.without_biometric_device" value="1" v-validate="foreignWorkerUnacceptableRule" data-vv-as="'Unacceptable(s)/Others'" :disabled="formDisabled" data-vv-as="Verification without using biometric device" id="unacceptable_fields[foreign_worker][without_biometric_device]">
                                        <input class="form-control" type="hidden" :class="{ 'is-invalid': errors.has('unacceptable_fields[foreign_worker][without_biometric_device]') }" />
                                        <label class="form-check-label" for="unacceptable_fields[foreign_worker][without_biometric_device]">Verification without using biometric device</label>
                                        <div class="invalid-feedback">{{ errors.first('unacceptable_fields[foreign_worker][without_biometric_device]') }}</div>
                                    </div>
                                    <div class="form-group col-sm-6 col-md-6">
