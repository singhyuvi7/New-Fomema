                                        name="unacceptable_fields[foreign_worker][no_medical_examination]"
                                        v-model="unacceptable_fields.foreign_worker.no_medical_examination"
                                        value="1"
                                        v-validate="foreignWorkerUnacceptableRule"
                                        data-vv-as="'Unacceptable(s)/Others'"
                                        :disabled="formDisabled"
                                        data-vv-as="No verification of identity prior to medical examination"
                                        id="unacceptable_fields[foreign_worker][no_medical_examination]"
