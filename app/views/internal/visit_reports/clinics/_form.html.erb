                                    :field_name="'visit_report_doctor[foreign_worker_present_acceptable]'"
                                    :rules="foreignWorkerPresentAcceptableRule"
                                    :label="'Yes'"
                                    :validation_label="'Acceptable'"
                                    :disabled="formDisabled"
                                    :id="'visit_report_doctor[foreign_worker_present_acceptable][yes]'"
                                ></v-radio>

