                                    v-model="unacceptable_fields.foreign_worker.using_photocopy_passport_comment"
                                    :field_name="'unacceptable_fields[foreign_worker][using_photocopy_passport_comment]'"
                                    :rules="{'required':(unacceptable_fields.foreign_worker.using_photocopy_passport)}"
                                    :label="''"
                                    :validation_label="'Comment'"
                                    :readonly="formReadOnly"
                                ></v-textarea>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="form-group col-sm-6 col-md-6">
                                    <input
                                        type="checkbox"
                                        name="unacceptable_fields[foreign_worker][using_id_card]"
