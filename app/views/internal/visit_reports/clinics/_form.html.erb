                                    <div class="invalid-feedback">{{ errors.first('unacceptable_fields[foreign_worker][others]') }}</div>
                                </div>
                                <div class="form-group col-sm-6 col-md-6">
                                    <v-textarea
                                    v-model="unacceptable_fields.foreign_worker.others_comment"
                                    :field_name="'unacceptable_fields[foreign_worker][others_comment]'"
                                    :rules="{'required':(unacceptable_fields.foreign_worker.others)}"
                                    :label="''"
