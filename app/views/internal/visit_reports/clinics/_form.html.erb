                                        <input class="form-control" type="hidden" :class="{ 'is-invalid': errors.has('unacceptable_fields[foreign_worker][using_photocopy_passport]') }"/>
                                    <label class="form-check-label" for="unacceptable_fields[foreign_worker][using_photocopy_passport]">Verification using photocopy passport</label>
