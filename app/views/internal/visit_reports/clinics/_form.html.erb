                                </div>
                            </div>
                            <div class="form-row">
                                <div class="form-group col-sm-6 col-md-6">
                                    <input type="checkbox"
                                        name="unacceptable_fields[apc][not_stated_in_place]"
                                        v-model="unacceptable_fields.apc.not_stated_in_place" value="1"
                                        v-validate="apcUnacceptableRule" data-vv-as="'Unacceptable(s)/Others'"
                                        :disabled="formDisabled"
                                        data-vv-as="Registered facility with FOMEMA not stated in Place of practice the APC"
                                        id="unacceptable_fields[apc][not_stated_in_place]">
