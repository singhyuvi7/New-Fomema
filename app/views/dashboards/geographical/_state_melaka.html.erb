<script>
function districtmelaka_map(dist){
$('#state-select').val(dist)
$('#divmelakaheader')[0].innerHTML=dist;
$("#divtotal_malaysialist").hide();
$('#div_melaka_states').show()
$.ajax({
url: "/dashboards/districts/" + dist,
dataType: "json",
success:function(data){

$('#divnoofmelakadoctors')[0].innerHTML=data.noofdoctors
$('#divnoofmelakaxray')[0].innerHTML=data.noofxray
$('#divnoofmelakalab')[0].innerHTML=data.nooflab
$('#divnoofmelakaemployer')[0].innerHTML=data.empcount
