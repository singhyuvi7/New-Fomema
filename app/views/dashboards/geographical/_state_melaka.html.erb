$('#noncommdiseasesmelaka')[0].innerHTML=data.noncommcount
$('#divmelakatuberculosis')[0].innerHTML=data.tuberculosis
$('#divmelakahepatisis')[0].innerHTML=data.hepatisis
$('#divmelakasyphilis')[0].innerHTML=data.syphilis
$('#divmelakahiv')[0].innerHTML=data.hiv
$('#divmelakamalaria')[0].innerHTML=data.malaria
$('#divmelakaleprosy')[0].innerHTML=data.leporacy

$('#divmelakapreg')[0].innerHTML=data.pregnancy
$('#divmelakaurine')[0].innerHTML=data.urine
$('#divmelakacannabis')[0].innerHTML=data.cannabis
$('#divmelakaillness')[0].innerHTML=data.illness
$('#divmelakaepilepsy')[0].innerHTML=data.epilsey
$('#divmelakacancer')[0].innerHTML=data.cancer
$('#divmelakahyper')[0].innerHTML=data.hypertension
$('#divmelakadiabetes')[0].innerHTML=data.diabetes
$('#divmelakakidney')[0].innerHTML=data.kidney
