$('#divmelakaasthama')[0].innerHTML=data.asthama
$('#divmelakaothers')[0].innerHTML=data.others
}
})

}
function district_melaka(dist){   
$('#district-select option:selected').text(dist)  
$('#divmelakaheader')[0].innerHTML=dist;
