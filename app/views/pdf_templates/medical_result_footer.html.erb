<script>
    function number_pages() {
    var vars={};
    var x=document.location.search.substring(1).split('&');
    for(var i in x) {var z=x[i].split('=',2);vars[z[0]] = unescape(z[1]);}
    var x=['frompage','topage','page','webpage','section','subsection','subsubsection'];
    for(var i in x) {
        var y = document.getElementsByClassName(x[i]);
        for(var j=0; j<y.length; ++j) y[j].textContent = vars[x[i]];
    }
    }
</script>

<body onload="number_pages()" class="text-center">
    <!--<div style="padding-bottom: 5px;"><b>Printed on <%#= "#{@print_datetime} by #{current_user.userable.code} (#{current_user.userable.name})"</b></div> %>-->
    <div class="page-counter"><i>Page: <span class="page"></span> of <span class="topage"></i></div>
    <span class="section"></span>
    <span class="subsection"></span>
    <span class="subsubsection"></span>
</body>
