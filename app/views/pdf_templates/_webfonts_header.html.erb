<style>
    @font-face {
        font-family: "Font Awesome 5 Brands";
        font-style: normal;
        font-weight: 400;
        font-display: block;
        src: url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-brands-400.eot' %>');
        src: url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-brands-400.eot?#iefix' %>') format("embedded-opentype"), 
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-brands-400.woff2' %>') format("woff2"), 
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-brands-400.woff' %>') format("woff"), 
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-brands-400.ttf' %>') format("truetype"), 
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-brands-400.svg#fontawesome' %>') format("svg")
    }.fab {
        font-family: "Font Awesome 5 Brands"
    }
    @font-face{
        font-family:"Font Awesome 5 Free";
        font-style:normal;
        font-weight:400;
        font-display:block;
        src:url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-regular-400.eot' %>');
        src:url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-regular-400.eot?#iefix' %>') format("embedded-opentype"),
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-regular-400.woff2' %>') format("woff2"),
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-regular-400.woff' %>') format("woff"),
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-regular-400.ttf' %>') format("truetype"),
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-regular-400.svg#fontawesome' %>') format("svg")
        }.fab, .far {
            font-weight: 400
        }
    @font-face{
        font-family:"Font Awesome 5 Free";
        font-style:normal;
        font-weight:900;
        font-display:block;
        src:url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-solid-900.eot' %>');
        src:url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-solid-900.eot?#iefix' %>') format("embedded-opentype"),
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-solid-900.woff2' %>') format("woff2"),
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-solid-900.woff' %>') format("woff"),
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-solid-900.ttf' %>') format("truetype"),
        url('<%= wicked_pdf_asset_path 'pdf/fontawesome/webfonts/fa-solid-900.svg#fontawesome' %>') format("svg")
    }.fa, .far, .fas {
        font-family: "Font Awesome 5 Free"
    }.fa, .fas {
        font-weight: 900
    }
</style>
